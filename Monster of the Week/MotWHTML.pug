	|  
	input.sheet-checkbox.sheet-ExpToggle(type='checkbox', name='attr_Toggle-Basics', value='1', checked='')
	span.sheet-anglebox
	|  
	span.sheet-ExpHeader
	.sheet-Expander
		.sheet-3colrow
			.sheet-col
				span.sheet-Label(data-i18n='character-name') Name
				|  
				br
				input(type='text', name='attr_character_name')
			input.sheet-SkinCheck(type='hidden', name='attr_SkinCheck', value='0')
			.sheet-col.sheet-NoKeep
				span.sheet-Label(data-i18n='character-gender') Pronouns
				|  
				br
				input(type='text', name='attr_Gender')
			.sheet-col
				include includes/sheet-selection-dropdown.pug
				input.sheet-SkinCheck(type='hidden', name='attr_SkinCheck', value='0')
				.sheet-Teams
					include includes/team-selection-dropdown.pug
		input.sheet-SkinCheck(type='hidden', name='attr_SkinCheck', value='0')
		.sheet-NoKeep
			span.sheet-Label(data-i18n='character-look') Look
			|  
			br
			input(type='text', name='attr_Look', style='width: 98%')
		include includes/game-options-config.pug
		// Here goes the top of the keeper's mystery sheet; more to follow
		include includes/keeper-mystery-head.pug
	// Screen that shows when the value is null because no playbook has been selected
	include includes/sheet-selection-long.pug
	br
